        window.onload = function() {

            var mapelement = document.getElementById('map');

            if (mapelement) {

                function initialize() {

                    var locations = [
                        ['Solutecip', -12.1192666,-77.0290238, 2],
                        ['Solutecip', -12.1192666,-77.0290238, 1]
                    ];
                    var styles = [
                        {
                            "featureType": "landscape",
                            "stylers": [
                                {
                                    "saturation": -100
                                },
                                {
                                    "lightness": 65
                                },
                                {
