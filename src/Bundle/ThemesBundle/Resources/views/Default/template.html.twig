        </a>
    </div>
    <![endif]-->

    </head>

    <body class="">

        {% block content %}

        {% endblock %}

    </body>

{% block javascripts %}

    {% javascripts
        '@ThemesBundle/Resources/public/default/js/jquery-2.1.1.min.js'
        '@ThemesBundle/Resources/public/default/js/bootstrap.min.js'
