        '@ThemesBundle/Resources/public/default/js/owl-carousel/owl.carousel.min.js'

    %}
    <script src="{{ asset_url }}"></script>

    {% endjavascripts %}

    <script src="https://maps.googleapis.com/maps/api/js?key=xxxx&v=3.exp&signed_in=true"></script>

