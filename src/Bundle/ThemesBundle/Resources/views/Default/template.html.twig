<html dir="ltr" lang="en">
<!--<![endif]-->

<head>

    {{ head.meta_default() }}
    {{ head.favicon() }}
    {{ head.canonical() }}

    {% block seo %}
        {% block head %}

            {#para copiar en otras paginas#}
            {{ head.title() }}
        {% endblock %}

        {% block twitter %}
            {{ twitter.card() }}

            {#para copiar en otras paginas#}
            {#{{ twitter.title() }}#}
            {#{{ twitter.description() }}#}
            {#{{ twitter.image() }}#}
        {% endblock %}

        {% block open_graph %}
            {{ og.default() }}
            {{ og.url() }}

            {#para copiar en otras paginas#}
            {#{{ og.title() }}#}
            {#{{ og.description() }}#}
            {#{{ og.image() }}#}
        {% endblock %}

        {% block bi3d %}
            {#para copiar en otras paginas#}
            {#{{ bi3d.art_id() }}#}
            {#{{ bi3d.section() }}#}
            {#{{ bi3d.art_title() }}#}
            {#{{ bi3d.pub_date() }}#}
        {% endblock %}
    {% endblock %}

    {% block stylesheets %}
        {% stylesheets
            filter='cssrewrite'
            'bundles/themes/default/css/bootstrap.min.css'
            '@ThemesBundle/Resources/public/default/css/font-awesome.min.css'
            '@ThemesBundle/Resources/public/default/css/font-awesome.min.css'
            '@ThemesBundle/Resources/public/default/css/jquery.fancybox.css'
            '@ThemesBundle/Resources/public/default/css/owl-carousel/owl.carousel.css'
            '@ThemesBundle/Resources/public/default/css/livesearch.css'
            '@ThemesBundle/Resources/public/default/css/photoswipe.css'
            '@ThemesBundle/Resources/public/default/css/jquery.bxslider.css'
            '@ThemesBundle/Resources/public/default/css/stylesheet.css'
            '@ThemesBundle/Resources/public/default/css/superfish.css'
            '@ThemesBundle/Resources/public/default/css/responsive.css'
            '@ThemesBundle/Resources/public/default/css/parallax.css'
            '@ThemesBundle/Resources/public/default/css/camera.css'
            '@ThemesBundle/Resources/public/default/css/owl-carousel/owl.carousel.css'
            '@ThemesBundle/Resources/public/default/css/owl-carousel/owl.transitions.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock %}

    <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700,400italic&subset=latin,cyrillic' rel='stylesheet' type='text/css'>

    <!--[if lt IE 9]>
    <div style='clear:both;height:59px;padding:0 15px 0 15px;position:relative;z-index:10000;text-align:center;'>
